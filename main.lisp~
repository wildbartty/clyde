(in-package :common-lisp-user)

(defpackage clim-test
  (:use #:clim #:clim-lisp)
  (:export app-main))

(in-package :clim-test)

(define-application-frame superapp ()
  ()
  (:panes
   (int :interactor :height 400 :width 600))
  (:layout
   (default int)))

(defun app-main ()
  (run-frame-top-level (make-application-frame 'superapp))
